<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/editcontactV"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ListView
        android:id="@+id/listviewdb"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:textColor="@android:color/black">

    </ListView>
    <com.github.clans.fab.FloatingActionMenu

        android:id="@+id/fabmenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="35dp"
        android:layout_marginEnd="16dp"
        fab:menu_animationDelayPerItem="55"
        fab:menu_backgroundColor="@android:color/transparent"
        fab:menu_buttonSpacing="0dp"
        fab:menu_colorNormal="#000000"
        fab:menu_colorPressed="#4DD0E1"
        fab:menu_colorRipple="#99d4d4d4"
        fab:menu_fab_label="Edit Options"
        fab:menu_fab_size="normal"
        fab:menu_icon="@drawable/ic_deletesw"
        fab:menu_labels_colorNormal="#333"
        fab:menu_labels_colorPressed="#444"
        fab:menu_labels_colorRipple="#66efecec"
        fab:menu_labels_cornerRadius="3dp"
        fab:menu_labels_ellipsize="none"
        fab:menu_labels_hideAnimation="@anim/fab_slide_out_to_right"
        fab:menu_labels_margin="0dp"
        fab:menu_labels_maxLines="-1"
        fab:menu_labels_padding="8dp"
        fab:menu_labels_position="left"
        fab:menu_labels_showAnimation="@anim/fab_slide_in_from_right"
        fab:menu_labels_showShadow="true"
        fab:menu_labels_singleLine="false"
        fab:menu_labels_textColor="#f2f1f1"
        fab:menu_labels_textSize="15sp"
        fab:menu_openDirection="up"
        fab:menu_shadowColor="#64B5F6"
        fab:menu_shadowRadius="4dp"
        fab:menu_shadowXOffset="2dp"
        fab:menu_shadowYOffset="4dp"
        fab:menu_showShadow="true">


        <com.github.clans.fab.FloatingActionButton
            android:id="@+id/fabmenu1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_clear_all"
            fab:fab_label="Clear All Contacts"
            fab:fab_shadowColor="#EF9A9A"
            fab:fab_shadowRadius="4dp"
            fab:fab_shadowXOffset="4dp"
            fab:fab_shadowYOffset="4dp"
            fab:fab_showShadow="true"
            fab:fab_size="mini"
            tools:ignore="VectorDrawableCompat" />

        <com.github.clans.fab.FloatingActionButton
            android:id="@+id/fabmenu2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_delete_white"
            fab:fab_colorNormal="#26A69A"
            fab:fab_label="Delete Selected Contact(s)"
            fab:fab_shadowColor="#90CAF9"
            fab:fab_shadowRadius="4dp"
            fab:fab_shadowXOffset="4dp"
            fab:fab_shadowYOffset="4dp"
            fab:fab_showShadow="true"
            fab:fab_size="mini"
            tools:ignore="VectorDrawableCompat" />


    </com.github.clans.fab.FloatingActionMenu>

</RelativeLayout>